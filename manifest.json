{
    "manifest_version": 4,
    "type": "extension",
    "name": "Audio Sender",
    "slug": "audio_sender",
    "description": "一个用于直接以语音形式发送音频文件的插件",
    "version": "1.0.1",
    "authors": [{
        "name": "xtaw",
        "link": "https://github.com/xtaw"
    }],
    "repository": {
        "repo": "xtaw/LiteLoaderQQNT-Audio-Sender",
        "branch": "master",
        "release": {
            "tag": "1.0.1"
        }
    },
    "platform": ["win32", "linux", "darwin"],
    "injects": {
        "main": "./src/main.js",
        "preload": "./src/preload.js",
        "renderer": "./src/renderer.js"
    }
}